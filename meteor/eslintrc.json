{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",

    },

    "rules": {
        "prefer-destructuring": ["error", {
            "VariableDeclarator": {
                "array": true,
                "object": true
            },
            "AssignmentExpression": {
                "array": true,
                "object": true
            }
        }, {
            "enforceForRenamedProperties": false
        }]
    }
}